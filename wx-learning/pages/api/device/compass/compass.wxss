.container {
  display: block;
  padding: 0;
  height: 100%;
  font-family: "microsoft yahei";
}
page {
  width: 100%;
  height: 100%;
  background-color: #011b28;
}
.text {
  color: white;
  font-size: 26rpx;
  text-align: center;
  padding-top: 10vh;
}

.text text {
  display: flex;
  flex-direction: column;
  height: 40rpx;
  line-break: 40rpx;
}

.img {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.tip-btn{
  width: 80% !important;
  margin-top: 10vh !important;
}



