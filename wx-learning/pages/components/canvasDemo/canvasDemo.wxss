page {
  width: 100%;
  height: 100%;
}

.my-container {
  width: 100%;
  height: 100%;
}

.canvas-area {
  width: 100%;
  height: 100%;
  background-color: #f8f8f8;
}

.my-canvas {
  width: 100%;
  height: 100%;
}

.canvas-tools {
  width: 100%;
  height: 100rpx;
  position: fixed;
  left: 0;
  bottom: 20rpx;
  display: flex;
  justify-content: space-between;
}

.box {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: rebeccapurple;
}

.box1 {
  background-color: #9cc;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}

.box2 {
