/* pages/eventDemo/eventDemo.wxss */
.event-container {
  height: 100%;
  display: flex;
}

.page {
  width: 100%;
  height: 100%;
}

.nav {
  display: flex;
  height: 50rpx;
  white-space: nowrap;
}

.nav navigator {
  margin: 0 14rpx;
  height: 20rpx;
  font-weight: bold;
}

.bg-red {
  background-color: red;
}

.bg-green {
  background-color: green;
}

.bg-yellow {
  background-color: yellow;
}

.nav navigator.active {
  color: blue;
}